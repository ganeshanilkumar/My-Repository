{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "contentscript.js" ],
      "matches": [ "https://*.salesforce.com/*", "https://*.force.com/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Overrides the standard salesforce favicon with one colored based on the org. Also, displays a separate icon for sandbox instances.",
   "icons": {
      "128": "icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Salesforce Colored Favicons",
   "options_page": "options.html",
   "permissions": [ "http://*/*", "tabs", "https://*/*", "chrome://favicon/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.1"
}
